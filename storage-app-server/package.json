{
  "name": "server",
  "version": "5.0",
  "main": "app.ts",
  "scripts": {
    "start": "NODE_ENV=production ts-node -r dotenv/config src/app.ts dotenv_config_path=.env.remote",
    "dev": "NODE_ENV=development ts-node -r dotenv/config src/app.ts",
    "build": "tsc",
    "lint": "eslint . --max-warnings 9999",
    "lint:strict": "eslint . --max-warnings 0",
    "test": "jest",
    "format": "prettier --write \"src/**/*.{js,ts,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,ts,json,md}\"",
    "format:all": "prettier --write \"./**/*.{js,ts,json,md}\" --ignore-path .prettierignore",
    "migrate": "node scripts/migrate-schema.js",
    "test:schema": "node scripts/test-schema.js",
    "check:db": "node scripts/check-mongodb.js",
    "create:test-data": "node scripts/create-test-data.js",
    "clean:test-data": "node scripts/clean-test-data.js",
    "test:material-unloading": "node scripts/test-material-unloading.js",
    "test:xlsx-export": "node scripts/test-xlsx-export.js",
    "fix:material-unloading": "node scripts/fix-material-unloading.js"
  },
  "keywords": [],
  "author": {
    "name": "Antonio Guiotto",
    "email": "powerhydratoni@gmail.com"
  },
  "license": "ISC",
  "dependencies": {
    "@faker-js/faker": "^8.4.1",
    "@types/bcrypt": "^5.0.2",
    "@types/exceljs": "^0.5.3",
    "axios": "^1.11.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "exceljs": "^4.4.0",
    "express": "^4.19.2",
    "express-validator": "^7.2.0",
    "json2csv": "^6.0.0-alpha.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.6.0",
    "recharts": "^2.12.7"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/json2csv": "^5.0.7",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^22.5.1",
    "@typescript-eslint/eslint-plugin": "^8.5.0",
    "@typescript-eslint/parser": "^8.5.0",
    "eslint": "^9.10.0",
    "nodemon": "^3.1.4",
    "prettier": "^3.3.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  },
  "description": ""
}
